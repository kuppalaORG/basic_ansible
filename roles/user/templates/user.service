[Unit]
Description = User Service
[Service]
User=roboshop
Environment=MONGO=true
Environment=REDIS_URL='redis://redis.codedeploywithbharath.tech:6379'
Environment=MONGO_URL="mongodb://mongod.codedeploywithbharath.tech:27017/users"
ExecStart=/bin/node /app/server.js
SyslogIdentifier=user

[Install]
WantedBy=multi-user.target